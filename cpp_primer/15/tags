Item_Base	tags	/^Item_Base	item.h	\/^        Item_Base(const string nm, const double pr):isbn(nm),price(pr) {$\/;"	f	class:Item_Base	access:public	signature:(const string nm, const double pr)$/;"	f	signature:(const string nm, const double pr)
Item_Base::Item_Base	item.h	/^        Item_Base(const string nm, const double pr):isbn(nm),price(pr) {$/;"	f	class:Item_Base	access:public	signature:(const string nm, const double pr)
Item_Base::isbn	item.h	/^        string isbn;$/;"	m	class:Item_Base	access:private
Item_Base::net_price	item.h	/^        double net_price(const size_t num) {$/;"	f	class:Item_Base	access:public	signature:(const size_t num)
Item_Base::price	item.h	/^        double price;$/;"	m	class:Item_Base	access:private
Item_Base::~Item_Base	item.h	/^        virtual ~Item_Base() { $/;"	f	class:Item_Base	access:public	signature:()
